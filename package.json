    "aria-autocomplete",
    "REST",
    "Search",
    "list",
    "Webpack",
    "customizable",
    "controllable",
    "component",
    "hello to tag readers",
    "feature-rich",
    "simple",
    "API",
    "explained",
    "comprehensive",
    "config-less",
    "zero-configuration",
    "versatile",
    "JavaScript",
    "JS"
  ],
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
